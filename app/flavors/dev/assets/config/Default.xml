<?xml version="1.0" encoding="utf-8"?>
<!-- 
	文件名： Default.xml
	描述：
		本文件记录项目通用的参数设置，通过DefaultConfig来读取
-->

<config>
    <!-- 是否调试,影响到日志打印、环境切换等 -->
    <debug>true</debug>
    <!-- app标识，用于创建文件夹名称等 -->
    <appid>LSSport</appid>
    <!-- 模式，支持值：dev,qa,release -->
    <mode>dev</mode>
    <!-- 数据库版本号，有表数据变化时需修改 -->
    <database_version>2</database_version>
    <!-- 协议配置文件名 -->
    <!--<protocol_config_name>ProtocolConfig_${mode}.xml</protocol_config_name>-->

    <!-- 协议配置文件名 因dev环境问题较多，现在默认把dev的环境qa2 -->
    <protocol_config_name>ProtocolConfig_qa2.xml</protocol_config_name>

    <!-- 渠道 -->
    <build_channel>default</build_channel>
    <UMENG_APPKEY>56cff29867e58e99030018c1</UMENG_APPKEY>
    <appType>6</appType>


</config>